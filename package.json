    "start": "nuxt start",
    "precommit": "lint-staged",
    "update:check": "ncu",
    "update:update": "ncu -u",
    "test": "jest"
  },
  "lint-staged": {
    "*.ts": "yarn lint:ts",
    "*.tsx": "yarn lint:ts"
  },
